<?xml version="1.0"?>

<launch>
    <!--- Run Gazebo simulation -->
    <include file="$(find husky_gazebo)/launch/husky_playpen.launch"/>
    
    <!--- Open rviz -->
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find husky_localization)/rviz/mapless.rviz" output="screen"/>
  
    <!--- Run AMCL -->
    <include file="$(find husky_navigation)/launch/move_base_mapless.demo.launch" />
  
  
</launch>